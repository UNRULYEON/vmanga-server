{
  "name": "vmanga-server",
  "version": "1.0.0",
  "description": "vmanga server for reading manga",
  "main": "./build/index.js",
  "repository": "git@github.com:UNRULYEON/vmanga-server.git",
  "author": "Amar Kisoensingh <amar@kisoensingh.sh>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
